class ShapeTracker {
    
    /* Tracker of line count, shape, weight and color
    */
    public int line_tracker, shape, line_weight, line_color;
    
    public ShapeTracker (int a, int b, int c, int d) {
        line_tracker = a;
        shape = b;
        line_weight = c;
        line_color = d;
    }
}
