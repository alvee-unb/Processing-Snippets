class Line {
    public float x1, y1, x2, y2;
    
    public Line (float a, float b, float c, float d) {
        x1 = a;
        y1 = b;
        x2 = c;
        y2 = d;
    }
    
    public float getx1() {
        return x1;
    }
    public float gety1() {
        return y1;
    }
    public float getx2() {
        return x2;
    }
    public float gety2() {
        return y2;
    }
}
